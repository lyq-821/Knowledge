/* 文字居中，左右显示 */
.text-align-center{
    text-align: center;
}
.text-align-left{
    text-align: left;
}
.text-align-right{
    text-align: right;
}
/* 文字省略号显示 -webkit-line-clamp: 2;写在响应容器控制显示的行数*/
.text-overflow-num{
    display: -webkit-box;
    overflow: hidden;
    text-overflow: ellipsis;
    word-wrap: break-word;
    white-space: normal !important;
    -webkit-box-orient: vertical;
}
/* 定位问题 */
.relative{
    position: relative;
}
.absolute{
    position: absolute;
}
